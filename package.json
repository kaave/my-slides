{
  "name": "webpack-hmr",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "flow-typed install && rimraf flow-typed/npm/browser-env_*",
    "start": "run-p test:watch build:development",
    "server": "http-server ./build -p 23000 -o --cors",
    "prebuild": "run-p lint test",
    "build": "run-s clean build:production",
    "build:development": "NODE_ENV=development webpack-dev-server",
    "build:production": "NODE_ENV=production webpack",
    "build:emergency": "NODE_ENV=production webpack",
    "build:check": "run-s build:production server",
    "clean": "rimraf build",
    "deploy": "simple-sftp-deploy",
    "lint": "run-p typecheck lint:scripts lint:styles",
    "lint:scripts": "eslint --ext .js,.jsx source/scripts/",
    "lint:scripts:fix": "eslint --fix --ext .js,.jsx source/scripts/",
    "lint:styles": "stylelint 'source/**/*.scss'",
    "test": "NODE_ENV=test ava",
    "test:watch": "NODE_ENV=test ava --watch",
    "typecheck": "flow",
    "flow-typed": "flow-typed"
  },
  "ava": {
    "files": [
      "source/scripts/**/*.test.{js,jsx}"
    ],
    "source": [
      "source/scripts/**/*.{js,jsx}"
    ],
    "require": [
      "babel-register",
      "./source/scripts/_test-helper.js"
    ],
    "babel": "inherit"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-date-fns": "^0.1.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "browser-env": "^2.0.31",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "ejs-compiled-loader": "^1.1.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-flowtype-errors": "^3.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2 || ^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.45.0",
    "flow-typed": "^2.1.2",
    "flowtype-loader": "^0.2.2",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.10.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "postcss-flexbugs-fixes": "^2.1.1",
    "postcss-loader": "^2.0.2",
    "postcss-nth-child-fix": "^1.0.0",
    "react-hot-loader": "^1.3.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "simple-sftp-deploy": "git+ssh://git@framelunch.com:framelunch/simple-sftp-deploy.git",
    "ssh2": "^0.5.4",
    "style-loader": "^0.17.0",
    "stylelint": "^7.10.0",
    "stylelint-config-sass-guidelines": "^2.1.0",
    "stylelint-scss": "^1.4.4",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "date-fns": "^1.28.4",
    "dotenv": "^4.0.0",
    "font-awesome": "^4.7.0",
    "react": "^15.5.4",
    "react-bem-helper": "^1.4.1",
    "react-dom": "^15.5.4",
    "rxjs": "^5.3.1",
    "sanitize.css": "^5.0.0"
  }
}
